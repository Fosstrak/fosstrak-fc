    <wsdl:operation name="getTMSpec">
      <wsdlsoap:operation soapAction=""/>
      <wsdl:input name="getTMSpecRequest">
        <wsdlsoap:body use="literal"/>
      </wsdl:input>
      <wsdl:output name="getTMSpecResponse">
